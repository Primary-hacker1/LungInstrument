<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <style name="Theme.App_test" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <declare-styleable name="CircleProgressBar">
        <attr format="color" name="cpb_background"/>
        <attr format="color" name="txtColor"/>
        <attr format="color" name="progressColorBackground"/>
        <attr format="color" name="progressColor"/>
        <attr format="integer" name="progressStrokeWidth"/>
        <attr format="integer" name="max"/>
        <attr format="integer" name="progress"/>
    </declare-styleable>
</resources>