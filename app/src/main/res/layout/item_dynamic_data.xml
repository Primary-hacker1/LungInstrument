<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
                name="bean"
                type="com.just.machine.dao.lung.CPXBreathInOutData" />
    </data>

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <TextView
                android:text="@{bean.createTime}"
                android:layout_width="90dp"
                android:gravity="center"
                android:background="#f4f5fa"
                android:maxLines="1"
                android:layout_height="30dp" />

        <TextView
                android:text="@{String.valueOf(bean.VTin)}"
                android:layout_width="90dp"
                android:gravity="center"
                android:background="#f4f5fa"
                android:maxLines="1"
                android:layout_height="30dp" />

        <TextView
                android:text="@{String.valueOf(bean.VCO2)}"
                android:layout_width="90dp"
                android:gravity="center"
                android:background="#f4f5fa"
                android:maxLines="1"
                android:layout_height="30dp" />

        <TextView
                android:text="@{String.valueOf(bean.RER)}"
                android:layout_width="90dp"
                android:gravity="center"
                android:background="#f4f5fa"
                android:maxLines="1"
                android:layout_height="30dp" />

        <TextView
                android:text="@{String.valueOf(bean.VO2_div_HR)}"
                android:layout_width="90dp"
                android:gravity="center"
                android:background="#f4f5fa"
                android:maxLines="1"
                android:layout_height="30dp" />

    </LinearLayout>
</layout>